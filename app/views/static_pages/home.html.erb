<h1>StaticPages#home</h1>
<p>Find me in app/views/static_pages/home.html.erb</p>
<% if user_signed_in? %>
    <h3>Hello <%= current_user.email %> </h3>
    <p><%= link_to "log out", destroy_user_session_path, method: :delete %> <%= link_to "edit profile", edit_user_registration_path %></p>
<% else %>
    <p><%= link_to "sign in", new_user_session_path %> <%= link_to "sign up", new_user_registration_path %></p>
<% end %>